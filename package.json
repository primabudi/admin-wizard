{
  "name": "admin-wizard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "api:step1": "json-server --watch mock-api/db-step1.json --port 4001 --middlewares mock-api/middleware.cjs",
    "api:step2": "json-server --watch mock-api/db-step2.json --port 4002 --middlewares mock-api/middleware.cjs",
    "docker:start": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.28.0",
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "react-select": "^5.10.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^4.0.5",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^24.1.3",
    "json-server": "^0.17.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.5"
  }
}
